# AvaAgent Contracts Environment Variables
# Copy this file to .env and fill in your values

# =============================================================================
# REQUIRED: Deployer Configuration
# =============================================================================

# Private key for deployment (without 0x prefix)
# ⚠️ NEVER commit real private keys! Use a dedicated testnet wallet.
PRIVATE_KEY=

# =============================================================================
# OPTIONAL: API Keys for Contract Verification
# =============================================================================

# Snowtrace API key for Avalanche contract verification
# Get from: https://snowtrace.io/myapikey
SNOWTRACE_API_KEY=

# =============================================================================
# OPTIONAL: Custom RPC URLs
# =============================================================================

# Override default RPC URLs if needed
# AVALANCHE_FUJI_RPC=https://api.avax-test.network/ext/bc/C/rpc
# KITE_TESTNET_RPC=https://rpc-testnet.gokite.ai/

# =============================================================================
# DEPLOYMENT RESULTS (filled after deployment)
# =============================================================================

# Avalanche Fuji (Chain ID: 43113)
# FUJI_WALLET_FACTORY=
# FUJI_AGENT_REGISTRY=
# FUJI_PAYMENT_FACILITATOR=
# FUJI_INTENT_PROCESSOR=

